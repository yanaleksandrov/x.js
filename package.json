    "url": "git+https://github.com/yanaleksandrov/x.js.git"
  },
  "author": "Yan Aleksandrov",
  "license": "MIT",
  "scripts": {
    "watch": "webpack --mode development --watch",
    "start": "webpack serve --mode development --no-client-overlay-warnings",
